{
  "format_version": "1.20.10",
  "animation_controllers": {
    "controller.animation.kokyu.kaze_change": {
      "states": {
        "default": {
          "transitions": [
            { "kaze1": "query.skin_id == 15 && query.variant == 9 && query.is_sneaking" },
            { "kaze2": "query.skin_id == 15 && query.variant == 1 && query.is_sneaking" },
            { "kaze3": "query.skin_id == 15 && query.variant == 2 && query.is_sneaking" },
            { "kaze4": "query.skin_id == 15 && query.variant == 3 && query.is_sneaking" },
            { "kaze5": "query.skin_id == 15 && query.variant == 4 && query.is_sneaking" },
            { "kaze6": "query.skin_id == 15 && query.variant == 5 && query.is_sneaking" },
            { "kaze7": "query.skin_id == 15 && query.variant == 6 && query.is_sneaking" },
            { "kaze8": "query.skin_id == 15 && query.variant == 7 && query.is_sneaking" },
            { "kaze9": "query.skin_id == 15 && query.variant == 8 && query.is_sneaking" }
          ]
        },
        "kaze1":{
          "on_entry": [
            "/titleraw @s actionbar {\"rawtext\":[{\"text\":\"§2風の呼吸 壱ノ型\"}]}",
            "/event entity @s variant1"
          ],
          "transitions": [
            {"default":"!query.is_sneaking"}
          ]
        },
        "kaze2":{
