{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "kurokumaft:rabbit_helmet_white",
      "item": { "kurokumaft:orichalcum_boots": "query.owner_identifier == 'minecraft:player'" },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/kurokumaft/models/armor/rabbit_armor_white",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.rabbit_helmet"
      },
      "scripts": {
        "parent_setup": "variable.helmet_layer_visible = 0.0;",
        "animate": [
          {"lop_eared": "query.remaining_durability <= 20;"}
        ]
      },
      "animations": {
        "lop_eared": "animation.rabbit_helmet.lop_eared"
      },
      "render_controllers": [ "controller.render.armor" ]
    }
  }
}
