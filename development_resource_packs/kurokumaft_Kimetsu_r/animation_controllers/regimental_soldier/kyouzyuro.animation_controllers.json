					"animations" : [
						"kyouzyuro.san_1"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"hono4_1" : {
					"animations" : [
						"kyouzyuro.shi_1"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"hono5_1" : {
					"animations" : [
						"kyouzyuro.go_1"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				},
				"hono9_1" : {
					"animations" : [
						"kyouzyuro.ku_1"
					],
					"transitions" : [
						{"hono9_2":"query.any_animation_finished"}
					]
				},
				"hono9_2" : {
					"animations" : [
						"kyouzyuro.ku_2"
					],
					"transitions" : [
						{"hono9_3":"query.mark_variant == 2"}
					]
				},
				"hono9_3" : {
					"animations" : [
						"kyouzyuro.ku_2"
					],
					"transitions" : [
						{"default":"query.any_animation_finished"}
					]
				}
			}
		}
    }
