          ]
        },
        "kaze1_f":{
          "on_exit": [
            "/effect @s speed 0",
            "/event entity @s kurokumaft:random_kokyu_off",
            "/tag @s remove kokyu_kaze1_self"
          ],
          "transitions": [
            {"default":"variable.regimental_kaze = variable.regimental_kaze < 10 ? variable.regimental_kaze + 1:0;return variable.regimental_kaze == 0;"}
          ]
        },
