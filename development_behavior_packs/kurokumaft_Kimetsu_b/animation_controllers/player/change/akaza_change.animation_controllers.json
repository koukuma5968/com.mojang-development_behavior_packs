{
  "format_version": "1.20.30",
  "animation_controllers": {
    "controller.animation.kekkizyutu.akaza_change": {
      "states": {
        "default": {
          "transitions": [
            { "rashin": "query.skin_id == 52 && query.variant == 10 && query.is_sneaking" },
            { "kushiki": "query.skin_id == 52 && query.variant == 1 && query.is_sneaking" },
            { "ranshiki": "query.skin_id == 52 && query.variant == 2 && query.is_sneaking" },
            { "meshiki": "query.skin_id == 52 && query.variant == 3 && query.is_sneaking" },
            { "kamurosakiwari": "query.skin_id == 52 && query.variant == 4 && query.is_sneaking" },
            { "ryuseigunkou": "query.skin_id == 52 && query.variant == 5 && query.is_sneaking" },
