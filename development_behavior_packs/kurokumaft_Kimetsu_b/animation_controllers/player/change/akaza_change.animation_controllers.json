            { "kishinyaenshin": "query.skin_id == 52 && query.variant == 8 && query.is_sneaking" },
            { "aoginranzankou": "query.skin_id == 52 && query.variant == 9 && query.is_sneaking" }
          ]
        },
        "rashin":{
          "on_entry": [
            "/titleraw @s actionbar {\"rawtext\":[{\"text\":\"破壊殺・羅針\"}]}",
            "/event entity @s variant1"
          ],
          "transitions": [
            {"default":"!query.is_sneaking"}
