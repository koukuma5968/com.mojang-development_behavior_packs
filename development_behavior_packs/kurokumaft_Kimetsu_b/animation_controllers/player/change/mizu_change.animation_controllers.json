{
  "format_version": "1.20.10",
  "animation_controllers": {
    "controller.animation.kokyu.mizu_change": {
      "states": {
        "default": {
          "transitions": [
            { "mizu1": "query.skin_id == 11 && query.variant == 10 && query.is_sneaking" },
            { "mizu2": "query.skin_id == 11 && query.variant == 1 && query.is_sneaking" },
            { "mizu3": "query.skin_id == 11 && query.variant == 2 && query.is_sneaking" },
