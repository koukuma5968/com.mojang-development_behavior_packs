            { "mizu10": "query.skin_id == 11 && query.variant == 9 && query.is_sneaking" }
          ]
        },
        "mizu1":{
          "on_entry": [
            "/titleraw @s actionbar {\"rawtext\":[{\"text\":\"§9水の呼吸 壱ノ型\"}]}",
            "/event entity @s variant1"
