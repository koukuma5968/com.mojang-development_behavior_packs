{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.kokyu.sanemi_change": {
      "states": {
        "default": {
          "transitions": [
            { "sanemi1": "query.skin_id == 7 && query.variant == 9 && query.is_sneaking" },
            { "sanemi2": "query.skin_id == 7 && query.variant == 1 && query.is_sneaking" },
            { "sanemi3": "query.skin_id == 7 && query.variant == 2 && query.is_sneaking" },
            { "sanemi4": "query.skin_id == 7 && query.variant == 3 && query.is_sneaking" },
            { "sanemi5": "query.skin_id == 7 && query.variant == 4 && query.is_sneaking" },
