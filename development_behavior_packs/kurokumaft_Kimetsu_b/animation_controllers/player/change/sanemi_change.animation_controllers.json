            { "sanemi7": "query.skin_id == 7 && query.variant == 6 && query.is_sneaking" },
            { "sanemi8": "query.skin_id == 7 && query.variant == 7 && query.is_sneaking" },
            { "sanemi9": "query.skin_id == 7 && query.variant == 8 && query.is_sneaking" }
          ]
        },
        "sanemi1":{
          "on_entry": [
            "/titleraw @s actionbar {\"rawtext\":[{\"text\":\"§2風の呼吸 壱ノ型\"}]}",
            "/event entity @s variant1"
          ],
          "transitions": [
            {"default":"!query.is_sneaking"}
          ]
        },
        "sanemi2":{
