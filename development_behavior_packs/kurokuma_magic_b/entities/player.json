          ]
				}
			},

      "kurokumaft:fire_protection": {
				"run_command": {
					"command": [
					  "function magic/fire/armor/fire_helmet"
					]
				}
			},
      "kurokumaft:snow_defrosting": {
				"run_command": {
					"command": [
            "function magic/fire/armor/fire_chestplate"
          ]
				}
			},
			"kurokumaft:fire_strength": {
				"run_command": {
					"command": [
            "function magic/fire/armor/fire_leggings"
					]
				}
			},
			"kurokumaft:lava_walker": {
				"run_command": {
					"command": [
