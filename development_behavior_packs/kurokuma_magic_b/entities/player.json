				}
			},

      "kurokumaft:bow_drawing": {
				"add": {
          "component_groups": [ "kurokumaft:bow_drawing" ]
        }
			},
      "kurokumaft:shot_bow": {
				"remove": {
          "component_groups": [ "kurokumaft:bow_drawing" ]
