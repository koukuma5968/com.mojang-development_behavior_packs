{
  "format_version": "1.17.10",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:player",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,
      "scripts":{
        "animate":[
          "shield",
          "magic_bow",

          "fire","fire_change",
          "water","water_change",
          "lightning","lightning_change",
          "light","light_change",
          "dark","dark_change",
          "stone","stone_change",
          "snow","snow_change",
          "wind","wind_change",

          "flame","flame_change",
          "waterwave","waterwave_change",
          "brightness","brightness_change",
          "darkness","darkness_change",

          "blaze","blaze_change",
          "watercurrent","watercurrent_change",
          "storm","storm_change",
          
          "explosion","explosion_change",
          "firestorm","firestorm_change",
          "flamespark","flamespark_change"
        ]
      },
      "animations":{
        "shield":"controller.animation.armor.shield",
        "magic_bow": "controller.animation.magic.bow",

        "fire":"controller.animation.fire_magic.fire","fire_change": "controller.animation.fire_magic.fire_change",
        "water":"controller.animation.water_magic.water","water_change": "controller.animation.water_magic.water_change",
        "lightning":"controller.animation.lightning_magic.lightning","lightning_change": "controller.animation.lightning_magic.lightning_change",
        "light":"controller.animation.light_magic.light","light_change": "controller.animation.light_magic.light_change",
        "dark":"controller.animation.dark_magic.dark","dark_change": "controller.animation.dark_magic.dark_change",
        "stone":"controller.animation.stone_magic.stone","stone_change": "controller.animation.stone_magic.stone_change",
        "snow":"controller.animation.snow_magic.snow","snow_change": "controller.animation.snow_magic.snow_change",
        "wind":"controller.animation.wind_magic.wind","wind_change": "controller.animation.wind_magic.wind_change",

        "flame":"controller.animation.flame_magic.flame","flame_change": "controller.animation.flame_magic.flame_change",
        "waterwave":"controller.animation.water_magic.waterwave","waterwave_change": "controller.animation.water_magic.waterwave_change",
        "brightness":"controller.animation.linght_magic.brightness","brightness_change": "controller.animation.linght_magic.brightness_change",
        "darkness":"controller.animation.dark_magic.darkness","darkness_change": "controller.animation.dark_magic.darkness_change",

        "blaze":"controller.animation.fire_magic.blaze","blaze_change": "controller.animation.fire_magic.blaze_change",
        "watercurrent":"controller.animation.water_magic.watercurrent","watercurrent_change": "controller.animation.water_magic.watercurrent_change",
        "storm":"controller.animation.storm_magic.storm","storm_change": "controller.animation.storm_magic.storm_change",

        "explosion":"controller.animation.explosion_magic.explosion","explosion_change":"controller.animation.explosion_magic.explosion_change",
        "firestorm":"controller.animation.firestorm_magic.firestorm","firestorm_change":"controller.animation.firestorm_magic.firestorm_change",
        "flamespark":"controller.animation.flamespark_magic.flamespark","flamespark_change":"controller.animation.flamespark_magic.flamespark_change",

        "framepillar_ani": "animation.fire.framepillar","flarecircle_ani": "animation.fire.flarecircle","flamespark_ani": "animation.fire.flamespark",
        "waterpillar_ani": "animation.water.waterpillar","aqua_shot_ani": "animation.water.aqua_shot","tidal_wave_ani": "animation.water.tidal_wave",

        "fire_magic_bow_frame": "animation.magic_bow.fire_magic_bow_frame",
        "water_magic_bow_frame": "animation.magic_bow.water_magic_bow_frame",
        "wind_magic_bow_frame": "animation.magic_bow.wind_magic_bow_frame",
        "stone_magic_bow_frame": "animation.magic_bow.stone_magic_bow_frame",
        "ice_magic_bow_frame": "animation.magic_bow.ice_magic_bow_frame"

      }
    },

    "component_groups": {
      "minecraft:add_bad_omen": {
        "minecraft:spell_effects": {
          "add_effects": [
            {
              "effect": "bad_omen",
              "duration": 6000,
              "visible": false,
              "display_on_screen_animation": true
            }
          ]
        },
        "minecraft:timer": {
          "time": [ 0.0, 0.0 ],
          "looping": false,
          "time_down_event": {
            "event": "minecraft:clear_add_bad_omen",
            "target": "self"
          }
        }
      },
      "minecraft:clear_bad_omen_spell_effect": {
        "minecraft:spell_effects": {
        }
      },
      "minecraft:raid_trigger": {
        "minecraft:raid_trigger": {
          "triggered_event": {
            "event": "minecraft:remove_raid_trigger",
            "target": "self"
          }
        },
        "minecraft:spell_effects": {
          "remove_effects": "bad_omen"
        }
      },
      "kurokumaft:bow_drawing": {
        "minecraft:timer":{
          "time": 0.1,
          "time_down_event":{
            "triggers": [
              {
                "filters":{
                  "test": "has_tag",
                  "operator":"==",
                  "subject": "self",
                  "value": "bow_drawing"
                }
              }
            ],
            "event":"kurokumaft:shot_bow"
          }
        }
      },
      "kurokumaft:shot_bow":{
        "minecraft:spawn_entity": {
          "entities": [
            {
              "min_wait_time": 0,
              "max_wait_time": 0,
              "spawn_entity": "minecraft:arrow",
              "spawn_event": "minecraft:entity_spawned",
              "single_use": true,
              "num_to_spawn": 1,
              "should_leash": false
            }
          ]
        }
      },
      "variant0": {
        "minecraft:variant": {
            "value": 0
        }
      },
      "variant1": {
        "minecraft:variant": {
            "value": 1
        }
      },
      "variant2": {
        "minecraft:variant": {
            "value": 2
        }
      },
      "variant3": {
        "minecraft:variant": {
            "value": 3
        }
      }
    },

    "components": {
      "minecraft:experience_reward": {
        "on_death": "Math.Min(query.player_level * 7, 100)"
      },
      "minecraft:type_family": {
        "family": [ "player" ]
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement": {
        "value": 0.1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { 
              "all_of": [
                {"test": "has_equipment","domain": "torso","operator": "==","subject": "self","value": "kurokumaft:fire_magic_chestplate"},
                {"test": "in_lava", "subject": "self", "operator": "==", "value": true}
              ]
            },
            "cause": "lava",
            "damage_per_tick": 0
          },
          {
            "filters": { 
              "all_of": [
                {"test": "has_equipment","domain": "torso","operator": "!=","subject": "self","value": "kurokumaft:fire_magic_chestplate"},
                {"test": "in_lava", "subject": "self", "operator": "==", "value": true}
              ]
            },
            "cause": "lava",
            "damage_per_tick": 4
          }

        ]
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:player.saturation": {
        "value": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 4
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": false
      },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:insomnia": {
        "days_until_insomnia": 3
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame"
        ],
        "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.2, -0.1 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.2, -0.1 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:block_climber": {},
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "filters": {
                "all_of" :[
                  {"test": "has_tag","operator": "equals","subject": "self","value": "shield_guard"}
                ]
              }
            },
            "deals_damage": false,
            "on_damage_sound_event": "item.shield.block"
          }
        ]
      },
      "minecraft:environment_sensor": {
        "triggers": [
          {
            "filters": {
              "all_of": [
                {"test": "has_mob_effect","subject": "self","value": "bad_omen"},
                {"test": "is_in_village","subject": "self","value": true}
              ]
            },
            "event": "minecraft:trigger_raid"
          },

          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "hand","operator": "==","subject": "self","value": "kurokumaft:fire_magic_sword"}
              ]
            },
            "event": "kurokumaft:fire_magic_enchant"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "hand","operator": "==","subject": "self","value": "kurokumaft:ice_magic_sword"}
              ]
            },
            "event": "kurokumaft:ice_magic_enchant"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "hand","operator": "==","subject": "self","value": "kurokumaft:stone_magic_sword"}
              ]
            },
            "event": "kurokumaft:stone_magic_enchant"
          },

          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "head","operator": "==","subject": "self","value": "kurokumaft:ice_magic_helmet"}
              ]
            },
            "event": "kurokumaft:water_breathing_effect"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "torso","operator": "==","subject": "self","value": "kurokumaft:ice_magic_chestplate"}
              ]
            },
            "event": "kurokumaft:lava_freeze"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "leg","operator": "==","subject": "self","value": "kurokumaft:ice_magic_leggings"}
              ]
            },
            "event": "kurokumaft:fire_resistance"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "feet","operator": "==","subject": "self","value": "kurokumaft:ice_magic_boots"}
              ]
            },
            "event": "kurokumaft:ice_walker"
          },


          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "head","operator": "==","subject": "self","value": "kurokumaft:fire_magic_helmet"}
              ]
            },
            "event": "kurokumaft:fire_protection"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "torso","operator": "==","subject": "self","value": "kurokumaft:fire_magic_chestplate"}
              ]
              },
              "event": "kurokumaft:snow_defrosting"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "leg","operator": "==","subject": "self","value": "kurokumaft:fire_magic_leggings"}
              ]
            },
            "event": "kurokumaft:fire_strength"
          },
          {
            "filters": {
              "all_of": [
                {"test": "has_equipment","domain": "feet","operator": "==","subject": "self","value": "kurokumaft:fire_magic_boots"}
              ]
            },
            "event": "kurokumaft:lava_walker"
          }
        ]
      }
    },

    "events": {
      "minecraft:gain_bad_omen": {
        "add": {
          "component_groups": [
            "minecraft:add_bad_omen"
          ]
        }
      },
      "minecraft:clear_add_bad_omen": {
        "remove": {
          "component_groups": [
            "minecraft:add_bad_omen"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:clear_bad_omen_spell_effect"
          ]
        }
      },
      "minecraft:trigger_raid": {
        "add": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      },
      "minecraft:remove_raid_trigger": {
        "remove": {
          "component_groups": [ "minecraft:raid_trigger" ]
        }
      },

      "variant0": {
        "add": {
          "component_groups": [ "variant0" ]
        }
      },
      "variant1": {
        "add": {
          "component_groups": [ "variant1" ]
        }
      },
      "variant2": {
        "add": {
          "component_groups": [ "variant2" ]
        }
      },
      "variant3": {
        "add": {
          "component_groups": [ "variant3" ]
        }
      },

