{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.kokyu.kaminari": {
      "states": {
        "default": {
          "variables": {
            "kokyu_kaminari_attack": {
              "input": "variable.kokyu_kaminari_attack = (variable.kokyu_kaminari_attack ?? 0);"
            }
          },
          "transitions": [
            { "kaminari1_zenitu": "query.scoreboard('zenitu') == 1 && query.is_using_item" },
            { "kaminari1": "query.scoreboard('kaminari') == 1 && query.is_using_item" },
            { "kaminari2": "query.scoreboard('kaminari') == 2 && query.is_using_item" },
            { "kaminari3": "query.scoreboard('kaminari') == 3 && query.is_using_item" },
            { "kaminari4": "query.scoreboard('kaminari') == 4 && query.is_using_item" },
            { "kaminari5": "query.scoreboard('kaminari') == 5 && query.is_using_item" },
            { "kaminari6": "query.scoreboard('kaminari') == 6 && query.is_using_item" },
            { "kaminari7": "query.scoreboard('zenitu') == 7 && query.is_using_item" }
          ]
        },
        "kaminari1_zenitu":{
          "on_entry": [
            "/event entity @s variant1"
          ],
          "transitions": [
            {"kaminari1_zenitu2":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 10 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0;"}
          ]
        },
        "kaminari1_zenitu2":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6霹靂一閃\"}]}",
            "/effect @s speed 99999 5 true",
            "/scoreboard objectives add kaminari1 dummy",
            "/scoreboard players set @s kaminari1 0"
          ],
          "on_exit": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6\"},{\"score\":{\"name\":\"@s\",\"objective\":\"kaminari1\"}},{\"text\":\"連\"}]}",
            "/event entity @s variant0",
            "/effect @s speed 0",
            "/scoreboard players reset @s kaminari1",
            "/scoreboard objectives remove kaminari1"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 200 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0;"}
          ]
        },
        "kaminari1":{
          "on_entry": [
            "/event entity @s variant1"
          ],
          "transitions": [
            {"kaminari1_2":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 10 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0;"}
          ]
        },
        "kaminari1_2":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6霹靂一閃\"}]}",
            "/effect @s speed 99999 5 true",
            "/scoreboard objectives add kaminari1 dummy",
            "/scoreboard players set @s kaminari1 0"
          ],
          "on_exit": [
            "/event entity @s variant0",
            "/effect @s speed 0",
            "/scoreboard players reset @s kaminari1",
            "/scoreboard objectives remove kaminari1"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 200 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 || query.scoreboard('kaminari1') == 1;"}
          ]
        },
        "kaminari2":{
          "on_entry": [
            "/event entity @s variant2",
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6稲魂\"}]}",
            "/function kaminari/kaminari2"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 10 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        },
        "kaminari3":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6聚蚊成雷\"}]}",
            "/event entity @s variant3",
            "/function kaminari/kaminari2"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 50 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        },
        "kaminari4":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6遠雷\"}]}",
            "/event entity @s variant4"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 20 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        },
        "kaminari5":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6熱界雷\"}]}",
            "/event entity @s variant5"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 10 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        },
        "kaminari6":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6電轟雷轟\"}]}",
            "/event entity @s variant6"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 20 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        },
        "kaminari7":{
          "on_entry": [
            "/tellraw @s {\"rawtext\":[{\"text\":\"§6火雷神\"}]}",
            "/event entity @s variant7"
          ],
          "on_exit": [
            "/event entity @s variant0"
          ],
          "transitions": [
            {"default":"variable.kokyu_kaminari_attack = variable.kokyu_kaminari_attack < 10 ? variable.kokyu_kaminari_attack + 1:0;return variable.kokyu_kaminari_attack == 0 && !query.is_using_item;"}
          ]
        }
      }
    }
  }
}