{
  "format_version": "1.20.60",
  "minecraft:entity": {
    "description": {
      "identifier": "kurokumaft:lightning_hornet",
      "is_spawnable": true,
      "is_summonable": true,
      "scripts":{
        "animate":[
          "rider"
        ]
      },
      "animations":{
        "rider":"controller.animation.storm_eagle.rider",
        "takeoff":"animation.storm_eagle.takeoff",
        "landing":"animation.storm_eagle.landing"
      },
      "properties": {
        "minecraft:has_nectar": {
          "type": "bool",
          "client_sync": true,
          "default": "query.had_component_group('has_nectar')"
        }
      }
    },

    "component_groups": {
      "kurokumaft:wild": {
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:behavior.breed": {
          "priority": 4,
          "speed_multiplier": 1.0
        },
        "minecraft:behavior.melee_box_attack": {
          "priority": 2,
          "attack_once": true,
          "speed_multiplier": 1.4,
          "on_attack": {
            "event": "countdown_to_perish_event",
            "target": "self"
          }
        },
        "minecraft:attack": {
          "damage": 2
        },
        "minecraft:movement": {
          "value": 0.8
        },
        "minecraft:flying_speed": {
          "value": 0.2
        },
        "minecraft:jump.static": {
        },
        "minecraft:can_climb": {
        },
        "minecraft:tameable": {
          "probability": 1.0,
          "tame_items": [
            "carrot",
            "golden_carrot",
            "apple",
            "golden_apple"
          ],
          "tame_event": {
            "event": "minecraft:on_tame",
            "target": "self"
          }
        },
        "minecraft:behavior.tempt": {
          "priority": 5,
          "speed_multiplier": 1.2,
          "items": [
            "golden_apple",
            "appleEnchanted",
            "golden_carrot"
          ]
        },
        "minecraft:tamemount": {
          "min_temper": 0,
          "max_temper": 10,
          "feed_text": "action.interact.feed",
          "feed_items": [
            {
              "item": "apple",
              "temper_mod": 3
            },
            {
              "item": "carrot",
              "temper_mod": 5
            },
            {
              "item": "golden_carrot",
              "temper_mod": 10
            },
            {
              "item": "golden_apple",
              "temper_mod": 10
            },
            {
              "item": "appleEnchanted",
              "temper_mod": 10
            }
          ],
          "tame_event": {
            "event": "minecraft:on_tame",
            "target": "self"
          }
        }
      },
      "kurokumaft:tamed": {
        "minecraft:is_tamed": {
        },
        "minecraft:behavior.follow_owner": {
          "priority": 3,
          "speed_multiplier": 1.2,
          "start_distance": 10,
          "stop_distance": 16
        },
        "minecraft:movement": {
          "value": 0.8
        },
        "minecraft:rideable": {
          "seat_count": 2,
          "crouching_skip_interact": true,
          "family_types": [
            "player"
          ],
          "interact_text": "action.interact.ride.horse",
          "seats": {
            "position": [ 0.0, 0.5, -0.5 ]
          }
        },
        "minecraft:attack": {
          "damage": 4
        },
        "minecraft:strength": {
          "value": 100,
          "max": 200
        },
        "minecraft:inventory": {
          "inventory_size": 14,
          "container_type": "horse",
          "additional_slots_per_strength": 3
        },
        "minecraft:equippable": {
          "slots": [
            {
              "slot": 0,
              "item": "saddle",
              "accepted_items": [ "saddle" ],
              "on_equip": {
                "event": "minecraft:horse_saddled"
              },
              "on_unequip": {
                "event": "minecraft:horse_unsaddled"
              }
            }
          ]
        },
        "minecraft:horse.jump_strength": {
          "value": {
            "range_min": 0.0,
            "range_max": 0.0
          }
        },
        "minecraft:jump.static": {},
        "minecraft:can_power_jump": {
        }
      },
      "minecraft:horse_saddled": {
        "minecraft:is_saddled": {
        },
        "minecraft:input_ground_controlled": {
        },
        "minecraft:behavior.player_ride_tamed": {
        }
      }
    },

    "components": {
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:shooter": {
        "def": "kurokumaft:lightningboltmagic"
      },
      "minecraft:behavior.random_hover": {
        "priority": 12,
        "xz_dist": 8,
        "y_dist": 8,
        "y_offset": 3,
        "interval": 1,
        "hover_height": [ 1, 4 ]
      },
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      },
      "minecraft:balloonable": {
        "mass": 0.5
      },
      "minecraft:behavior.float": {
        "priority": 19
      },
      "minecraft:breathable": {
        "totalSupply": 15,
        "suffocateTime": 0
      },
      "minecraft:collision_box": {
        "width": 0.55,
        "height": 0.5
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:behavior.ranged_attack": {
        "priority": 2,
        "attack_interval": 5,
        "burst_interval": 0.2,
        "burst_shots": 3,
        "ranged_fov": 270,
        "attack_radius": 64,
        "charge_shoot_trigger": 2,
        "charge_charged_trigger": 1
      },
      "minecraft:type_family": {
        "family": [ "hornet", "mob", "arthropod", "familiar", "irongolem" ]
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": false
          },
          {
            "on_damage": {
              "filters": {
                "test": "is_block", "subject": "block", "value": "minecraft:sweet_berry_bush"
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:healable": {
        "items": [
          {
            "item": "wheat",
            "heal_amount": 2
          },
          {
            "item": "sugar",
            "heal_amount": 1
          },
          {
            "item": "hay_block",
            "heal_amount": 20
          },
          {
            "item": "apple",
            "heal_amount": 3
          },
          {
            "item": "golden_carrot",
            "heal_amount": 20
          },
          {
            "item": "golden_apple",
            "heal_amount": 20
          },
          {
            "item": "appleEnchanted",
            "heal_amount": 30
          }

        ]
      },
      "minecraft:behavior.random_stroll": {
        "priority": 7,
        "speed_multiplier": 1
      },
      "minecraft:behavior.look_at_player": {
        "priority": 8,
        "look_distance": 6,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 9
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1,
        "entity_types": [
          {
            "filters": { "test" :  "is_family", "subject" : "other", "value" :  "monster"},
            "max_dist": 32
          }
        ]  
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 5,
        "must_see": true,
        "entity_types": [
          {
            "filters": { "test" :  "is_family", "subject" : "other", "value" :  "monster"},
            "max_dist": 16
          }
        ]
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava", "subject": "self", "operator": "==", "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:navigation.hover": {
        "can_path_over_water": true,
        "can_sink": false,
        "can_pass_doors": false,
        "can_path_from_air": true,
        "avoid_water": true,
        "avoid_damage_blocks": true,
        "avoid_sun": false
      },
      "minecraft:movement.hover": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_fly": {
      },
      "minecraft:health": {
        "value": {
          "range_min": 80,
          "range_max": 150
        }
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    },

    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "kurokumaft:wild",
            "track_attacker"
          ]
        }
      },
      "minecraft:on_tame": {
        "remove": {
          "component_groups": [
            "kurokumaft:wild"
          ]
        },
        "add": {
          "component_groups": [
            "kurokumaft:tamed"
          ]
        }
      },
      "minecraft:horse_saddled": {
        "add": {
          "component_groups": [
            "minecraft:horse_saddled"
          ]
        }
      },
      "minecraft:horse_unsaddled": {
        "remove": {
          "component_groups": [
            "minecraft:horse_saddled"
          ]
        }
      }
    }
  }
}
