      "kurokumaft:gatling_shot_1": {
        "minecraft:shooter": {
          "def": "kurokumaft:twenty_two_lr_entity<kurokumaft:shot1>"
        }
      },
      "kurokumaft:gatling_shot_2": {
        "minecraft:shooter": {
          "def": "kurokumaft:twenty_two_lr_entity<kurokumaft:shot2>"
        }
      },
      "kurokumaft:gatling_shot_3": {
        "minecraft:shooter": {
          "def": "kurokumaft:twenty_two_lr_entity<kurokumaft:shot3>"
        }
      },
      "kurokumaft:gatling_shot_4": {
        "minecraft:shooter": {
          "def": "kurokumaft:twenty_two_lr_entity<kurokumaft:shot4>"
        }
      },
      "kurokumaft:gatling_shot_5": {
        "minecraft:shooter": {
          "def": "kurokumaft:twenty_two_lr_entity<kurokumaft:shot5>"
        }
      }
    },

    "components": {
      "minecraft:experience_reward": {
        "on_death": "Math.Min(query.player_level * 7, 100)"
      },
      "minecraft:type_family": {
        "family": [ "player" ]
      },
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:movement": {
        "value": 0.1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:attack": {
        "damage": 1
      },
      "minecraft:exhaustion_values": {
        "heal": 6,
        "jump": 0.05,
        "sprint_jump": 0.2,
        "mine": 0.005,
        "attack": 0.1,
        "damage": 0.1,
        "walk": 0.0,
        "sprint": 0.1,
        "swim": 0.01
      },
      "minecraft:player.saturation": {
        "value": 5,
         "max": 20
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 20
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": -1,
        "inhale_time": 3.75,
        "generates_bubbles": false
      },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      },
      "minecraft:physics": {
        "push_towards_closest_space": true
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": true
      },
      "minecraft:insomnia": {
        "days_until_insomnia": 3
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame"
        ],
        "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.2, -0.1 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.2, -0.1 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:block_climber": {},
      "minecraft:environment_sensor": {
        "triggers": [
          {
            "filters": {
              "all_of": [
                {"test": "has_mob_effect","subject": "self","value": "bad_omen"},
                {"test": "is_in_village","subject": "self","value": true}
              ]
            },
            "event": "minecraft:trigger_raid"
          },
          {
            "filters": {
              "any_of": [
                { "test": "actor_health", "subject": "self", "operator": "equals", "value": 6 }
              ]
            },
            "event": "kurokumaft:dying_damage"
          },
          {
            "filters": {
              "any_of": [
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:rabbit_helmet_brown"},
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:rabbit_helmet_white"}
              ]
            },
            "event": "kurokumaft:rabbit_jump_boost"
          },
          {
            "filters": {
              "any_of": [
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:wolf_helmet_white"},
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:wolf_helmet_black"}
              ]
            },
            "event": "kurokumaft:wolf_power_boost"
          },
          {
            "filters": {
              "any_of": [
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:chicken_helmet_white"}
              ]
            },
            "event": "kurokumaft:chicken_slow_falling"
          },
          {
            "filters": {
              "any_of": [
                {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:fox_helmet_orange"}
              ]
            },
            "event": "kurokumaft:fox_speed_boost"
          },
          {
            "filters": {
              "all_of": [
                {
                  "any_of": [
                    {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:axolotl_helmet_blue"},
                    {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:axolotl_helmet_cyan"},
                    {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:axolotl_helmet_gold"},
                    {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:axolotl_helmet_lucy"},
                    {"test": "has_equipment","domain": "head", "subject": "self","value": "kurokumaft:axolotl_helmet_wild"}
                  ]
                },
                { "test": "actor_health", "subject": "self", "operator": "<=", "value": 2 }
              ]
            },
            "event": "kurokumaft:axolotl_regeneration"
          },
          {
            "filters": {
              "all_of": [
                {
                  "any_of" :[
                    {"test": "has_tag","operator": "equals","subject": "self","value": "main_shield_guard"},
                    {"test": "has_tag","operator": "equals","subject": "self","value": "off_shield_guard"}
                  ]
                },
                {
                  "any_of": [
                    {"test": "has_equipment","domain": "hand", "subject": "self","value": "kurokumaft:steel_shield"}
                  ]
                }
              ]
            },
            "event": "kurokumaft:knockback_resist"
          },
          {
            "filters": {
              "any_of": [
                {
                  "all_of" :[
                    {"test": "has_tag","operator": "not","subject": "self","value": "main_shield_guard"},
                    {"test": "has_tag","operator": "not","subject": "self","value": "off_shield_guard"}
                  ]
                },
                {
                  "all_of": [
                    {"test": "has_equipment","domain": "hand","operator": "not","subject": "self","value": "kurokumaft:steel_shield"}
                  ]
                }
              ]
            },
            "event": "kurokumaft:knockback_resist_reset"
          }
        ]
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "filters": {
                "all_of" : [
                  {
                    "any_of" :[
                      {"test": "has_tag","operator": "equals","subject": "self","value": "main_shield_guard"},
                      {"test": "has_tag","operator": "equals","subject": "self","value": "off_shield_guard"}
                    ]
                  },
                  {
                    "any_of" :[
                      {"test": "has_damage","operator": "equals","subject": "self","value": "attack"},
                      {"test": "has_damage","operator": "equals","subject": "self","value": "projectile"},
                      {"test": "has_damage","operator": "equals","subject": "self","value": "sonic_boom"}
                    ]
                  }
                ]
              }
            },
            "cause": "all",
            "deals_damage": false
          },
          {
            "on_damage": {
              "filters": {
                "all_of" : [
                  {
                    "any_of" :[
                      {"test": "has_tag","operator": "equals","subject": "self","value": "main_shield_guard"},
                      {"test": "has_tag","operator": "equals","subject": "self","value": "off_shield_guard"}
                    ]
                  },
                  {
                    "any_of" :[
                      {"test": "has_damage","operator": "equals","subject": "self","value": "block_explosion"},
                      {"test": "has_damage","operator": "equals","subject": "self","value": "entity_explosion"}
                    ]
                  }
                ]
              }
            },
            "cause": "block_explosion",
            "damage_modifier": -10,
            "damage_multiplier": 2.0,
            "deals_damage": true
          }
        ]
      }

    },

    "events": {
