{
  "format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.bag.burdened": {
			"states": {
				"default": {
					"on_entry": [
						"/execute @p ~~~ say zero"
					],
					"transitions": [
						{ "one": "query.mark_variant == 1" }
					]
				},
				"one": {
					"on_entry": [
						"/execute @p ~~~ say one"
					],
					"transitions": [
						{ "default": "query.mark_variant == 0" },
						{ "two": "query.mark_variant == 2" }
					]
				},
				"two": {
					"on_entry": [
						"/execute @p ~~~ say one",
						"/event entity @s kurokumaft:stay"
					],
					"transitions": [
						{ "default": "(1.0)" }
					]
				}
			}
		}

	}
}